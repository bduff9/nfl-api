{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {},
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "plugins": [
    "import"
  ],
  "settings": {},
  "rules": {
    "indent": [
      "error",
      "tab", {
        "SwitchCase": 1,
        "VariableDeclarator": 2
      }
    ],
    "linebreak-style": "off",
    "no-console": "off",
    "no-constant-condition": "off",
    "space-before-function-paren": ["error", "always"],
    "no-unused-vars": [
      "warn", {
        "varsIgnorePattern": "UU",
        "args": "none"
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "import/named": 2
  }
}
